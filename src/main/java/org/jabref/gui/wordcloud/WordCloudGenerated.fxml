<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jabref.gui.wordcloud.WordCloudGeneratedView">
    <buttonTypes>
        <ButtonType fx:constant="CANCEL" />
        <ButtonType fx:id="downloadButton" buttonData="OK_DONE" text="%Download file" />
    </buttonTypes>
    <content>
        <VBox prefHeight="500.0" prefWidth="500.0" spacing="10.0">
            <ImageView fx:id="wordcloudGeneratedImageView" fitWidth="500.0" preserveRatio="true" />
        </VBox>
    </content>
</DialogPane>
